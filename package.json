{
  "name": "ember-select-light",
  "version": "2.0.5",
  "description": "The simple DDAU component built for Ember",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/ember-a11y/ember-select-light",
  "license": "MIT",
  "author": "Ava Wroten <ava@wroten.me> (https://www.wroten.me/)",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "ember-cli-babel": "^7.26.11",
    "ember-cli-htmlbars": "^6.2.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/string": "^3.0.1",
    "@ember/test-helpers": "^2.9.3",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "@babel/eslint-parser": "^7.21.3",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^2.6.1",
    "ember-cli": "^4.11.0",
    "ember-cli-dependency-checker": "^3.3.1",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-cli-update": "^1.0.1",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^1.0.0",
    "ember-page-title": "^7.0.0",
    "ember-qunit": "^6.2.0",
    "ember-resolver": "^10.0.0",
    "ember-source": "^4.11.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^5.7.1",
    "ember-try": "^2.0.0",
    "eslint": "^8.36.0",
    "eslint-plugin-ember": "^11.4.8",
    "eslint-plugin-node": "^11.1.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "qunit": "^2.19.4",
    "qunit-dom": "^2.0.0"
  },
  "engines": {
    "node": "16.* || 18.*"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
